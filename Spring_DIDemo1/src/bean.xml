<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
<bean id="emp" class="com.app.Employee">
<constructor-arg value="111" type="int"></constructor-arg>
<constructor-arg value="Rahul" ></constructor-arg>
<constructor-arg value="11000.09f" type="float" ></constructor-arg> 
<constructor-arg ref="date"></constructor-arg>
</bean>

<bean id="date" class="com.app.Date" >
<constructor-arg value="29" type="int"></constructor-arg>
<constructor-arg value="05" type="int"></constructor-arg>
<constructor-arg value="2023" type="int"></constructor-arg>
</bean>

</beans>